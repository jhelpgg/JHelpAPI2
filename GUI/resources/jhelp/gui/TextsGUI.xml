    <!-- Alpha part -->
    <Text key="colorChooser.alpha">
        Alpha
    </Text>
    <!-- Red part -->
    <Text key="colorChooser.red">
        Red
    </Text>
    <!-- Green part -->
    <Text key="colorChooser.green">
        Green
    </Text>
    <!-- Blue part -->
    <Text key="colorChooser.blue">
        Blue
    </Text>

